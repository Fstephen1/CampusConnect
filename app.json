{
  "expo": {
    "name": "Campus Connect",
    "slug": "campus-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Campus-Connect-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Campus-Connect-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E40AF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Campus-Connect-logo.png",
        "backgroundColor": "#1E40AF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/campus-connect-favicon.png",
      "name": "CampusConnect",
      "shortName": "CampusConnect",
      "lang": "en",
      "scope": "/",
      "themeColor": "#3b82f6",
      "backgroundColor": "#ffffff",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "preferRelatedApplications": false,
      "manifest": "./assets/manifest.json"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
